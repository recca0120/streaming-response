{
  "name": "recca0120/streaming-response",
  "description": "",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "recca0120",
      "email": "recca0120@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/filesystem": "^8.0|^9.0",
    "symfony/http-foundation": "^5.4|^6.0"
  },
  "require-dev": {
    "league/flysystem-aws-s3-v3": "^1.1|^3.0",
    "mockery/mockery": "^1.5",
    "orchestra/testbench": "^6.25|^7.19",
    "phpoption/phpoption": "^1.9",
    "phpunit/phpunit": "^8.5|^9.5"
  },
  "autoload": {
    "psr-4": {
      "Recca0120\\StreamingResponse\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Recca0120\\StreamingResponse\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Recca0120\\StreamingResponse\\StreamingResponseServiceProvider"
      ]
    }
  }
}
